{
  "name": "back-end",
  "version": "1.0.0",
  "main": "bin/www.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon bin/www",
    "develop": "nodemon -r dotenv/config bin/www",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:undo:all && sequelize db:seed:all",
    "db:setup": "yarn db:drop && yarn db:create && yarn db:migrate && yarn db:seed",
    "db:setup-prod": "yarn db:drop --env production && yarn db:create --env production && yarn db:migrate --env production && sequelize db:seed:undo:all --env production && sequelize db:seed:all --env production",
    "eslint": "eslint ./app",
    "test": "jest"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-status": "^1.7.4",
    "imagekit": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-schedule": "^2.1.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1",
    "sequelize-cli": "^6.6.2",
    "serverless-http": "^3.2.0",
    "swagger-ui-express": "^5.0.0",
    "vercel": "^34.2.7"
  },
  "devDependencies": {
    "crossenv": "^0.0.2-security",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4"
  },
  "author": "Fendy"
}
